      </ul>
    </div>
  </div><!-- /container -->
</div>
<!-- /Header -->

<!-- Main -->
<div class="container-fluid">
<div class="row">
  <div class="col-sm-3">
      <!-- Left column -->
      <a href="/test"><strong><i class="glyphicon glyphicon-wrench"></i>FAB-BUY</strong></a>  
      
      <hr>
      
      <ul class="list-unstyled">
        <li class="nav-header"> <a href="#" data-toggle="collapse" data-target="#userMenu">
          <h5>Settings <i class="glyphicon glyphicon-chevron-down"></i></h5>
          </a>
            <ul class="list-unstyled collapse in" id="userMenu">
                <li class="active"> <a href="/dyproducts"><i class="glyphicon glyphicon-home"></i> Home</a></li>
                <li><a href="/addtocart"><i class="glyphicon glyphicon-cog"></i> Cart <span class="badge badge-info"></span></a></li>
                <li><a href="#"><i class="glyphicon glyphicon-cog"></i> Phones</a></li>
                <li><a href="#"><i class="glyphicon glyphicon-cog"></i>Ipads</a></li>
                <li><a href="#"><i class="glyphicon glyphicon-cog"></i>Nailpolish</a></li>
                <li><a href="#"><i class="glyphicon glyphicon-off"></i> Logout</a></li>
            </ul>
        </li>
       
  
       
    </div><!-- /col-3 -->
    <div align="center" class="col-sm-9">          
<% if (productArray.length) { %>

<% productArray.forEach(function(productArray) { %>
<div class="card1">

<p>


<center><h4><%= JSON.stringify(productArray["id"]["S"])%></h4></center>
<center><h4><%= JSON.stringify(productArray["category"]["S"])%>--Category</h4></center>
<center><h4><%= JSON.stringify(productArray["brand"]["S"])%>--Brand</h4></center>
<center><h4><%= JSON.stringify(productArray["make"]["S"])%>--Make</h4></center>


</p>
<tr>
 <center>

<form action="/addtocart/<%= JSON.stringify(productArray["id"]["S"])%>/<%= JSON.stringify(productArray["category"]["S"]) %>/<%= JSON.stringify(productArray["cost"]["S"]) %>" method="post">

<input type="submit" value="Add to cart"></input>

</form> 

</center>
        </tr>


</div>



<% }); %>
<% } %>

  <img src="/images/collage.png" height="275px" width="600px">

